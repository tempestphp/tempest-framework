{
    "name": "tempest/support",
    "description": "A component for general support helper functions.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.4",
        "doctrine/inflector": "^2.0",
        "tempest/container": "1.0.0-alpha.6",
        "tempest/highlight": "^2.11.2",
        "voku/portable-ascii": "^2.0.3"
    },
    "autoload": {
        "psr-4": {
            "Tempest\\Support\\": "src"
        },
        "files": [
            "src/functions.php",
            "src/Str/functions.php",
            "src/Arr/functions.php",
            "src/Html/functions.php",
            "src/Random/functions.php",
            "src/Regex/functions.php",
            "src/Namespace/functions.php",
            "src/Language/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\Support\\Tests\\": "tests"
        }
    }
}
