    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.4",
        "tempest/core": "dev-main",
        "tempest/container": "dev-main",
        "tempest/validation": "dev-main",
        "tempest/clock": "dev-main",
        "league/commonmark": "^2.7",
