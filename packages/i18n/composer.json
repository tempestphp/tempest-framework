{
    "name": "tempest/i18n",
    "description": "A component for working with internationalization.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.4",
        "tempest/core": "dev-main",
        "tempest/container": "dev-main",
        "tempest/datetime": "dev-main"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5.17"
    },
    "autoload": {
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Tempest\\Internationalization\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\Internationalization\\Tests\\": "tests"
        }
    }
}
