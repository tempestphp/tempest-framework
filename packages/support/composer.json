{
    "name": "tempest/support",
    "description": "A component for general support helper functions.",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.4",
        "tempest/container": "2.7.2",
        "voku/portable-ascii": "^2.0.3",
        "symfony/uid": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "Tempest\\Support\\": "src"
        },
        "files": [
            "src/functions.php",
            "src/Str/functions.php",
            "src/Str/constants.php",
            "src/Arr/functions.php",
            "src/Html/functions.php",
            "src/Random/functions.php",
            "src/Regex/functions.php",
            "src/Namespace/functions.php",
            "src/Path/functions.php",
            "src/Comparison/functions.php",
            "src/Math/constants.php",
            "src/Math/functions.php",
            "src/Json/functions.php",
            "src/Filesystem/functions.php",
            "src/Uri/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\Support\\Tests\\": "tests"
        }
    }
}
