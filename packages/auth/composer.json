    "require": {
        "php": "^8.4",
        "tempest/core": "dev-main",
        "tempest/router": "dev-main",
        "tempest/database": "dev-main"
    },
    "require-dev": {
        "tempest/drift": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Tempest\\Auth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\Auth\\Tests\\": "tests"
        }
    },
    "license": "MIT",
    "minimum-stability": "dev"
}
