{
    "name": "tempest/debug",
    "description": "Debug functions for Tempest",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": "^8.5",
        "tempest/highlight": "^2.11.4",
        "symfony/var-dumper": "^7.1"
    },
    "require-dev": {
        "tempest/console": "dev-main"
    },
    "autoload": {
        "psr-4": {
            "Tempest\\Debug\\": "src"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tempest\\Debug\\Tests\\": "tests"
        }
    }
}
